{
    "manifest_version": 2,
    "name": "Automatic Multi Node Inspector",
    "version": "1.0",
    "description": "Allow you ton automaticly open multiple node inspector",
    "icons": { "128": "icon.png" },
    "browser_action": {
        "default_title": "",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "background",
        "http://localhost/*"
    ]
}